#!/bin/bash
#
#PBS -l ncpus=1
#PBS -l walltime=96:00:00
#
#PBS -q hyper
#
#PBS -M egentry@ucsc.edu
#PBS -m e
#
#PBS -S /bin/bash
#
cd $PBS_O_WORKDIR

source $HOME/.bashrc
time ../bin/SNe ../src/in.par.restart $RESTART_ID \
                                      $RESTART_N_CHECKPOINTS \
                                      $RESTART_DELTA_TIME \
                                      $RESTART_CFL \
                                      $RESTART_COOLING_REDSHIFT
